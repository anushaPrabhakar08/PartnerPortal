input {
  # Tail test.log
  file {
      path => "/Users/consulthys/workspaces/gz-grails/account-service/test.log"
      add_field => { "[@metadata][test]" => "Hello1" }
      codec => "json"
      type => "test"
  }
  # Tail test2.log
  file {
      path => "/Users/consulthys/workspaces/gz-grails/account-service/test2.log"
      add_field => { "[@metadata][test]" => "Hello2" }
      codec => "json"
      type => "test2"
  }
}
filter {
  if [type] == "test" {
    # Lowercase first name
    mutate {
      lowercase => "first_name"
    }
  }
  if [type] != "test2" {
    # Lowercase last name
    mutate {
      lowercase => "last_name"
    }
  }
}
output {
  if [type] < "1" {
    # Out
    stdout {
      codec => "rubydebug"
    }
  }
  if [type] <= "2" {
    # Out
    stdout {
      codec => "rubydebug"
    }
  }
  if [type] > "3" {
    # Out
    stdout {
      codec => "rubydebug"
    }
  }
  if [type] >= "4" {
    # Out
    stdout {
      codec => "rubydebug"
    }
  }
  if [type] >= 1 and [type] <= 10 {
    # Out
    stdout {
      codec => "rubydebug"
    }
  }
  if "123" in [type] {
    # Out
    stdout {
      codec => "rubydebug"
    }
  }
  if [type] =~ /ba.*/ {
    # Out
    stdout {
      codec => "rubydebug"
    }
  }
  if [type] {
    # Out
    stdout {
      codec => "rubydebug"
    }
  }
  if ![type] {
    # Out
    stdout {
      codec => "rubydebug"
    }
  }
  if ![type] {
    # Out
    stdout {
      codec => "rubydebug"
    }
  }
  if [type] {
    # Out
    stdout {
      codec => "rubydebug"
    }
  }
  if 1 == 1 {
    # Out
    stdout {
      codec => "rubydebug"
    }
  }
}